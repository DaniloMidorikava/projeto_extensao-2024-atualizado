program Projeto_OsIndependentes;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {Form1},
  Unit_responsavel in 'Unit_responsavel.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
